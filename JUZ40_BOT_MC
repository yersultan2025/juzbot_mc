    "–ö–æ–Ω—É—Å—Ç—ã“£ –∫”©–ª–µ–º—ñ —Ñ–æ—Ä–º—É–ª–∞": r"V = \frac{1}{3} \pi r^2 h",
    "–ö–æ–Ω—É—Å –∫”©–ª–µ–º —Ñ–æ—Ä–º—É–ª–∞": r"V = \frac{1}{3} \pi r^2 h"   
  }

# üìå –§–æ–Ω —Ç“Ø—Å—ñ
BACKGROUND_COLOR = "#0097A7"
IMG_SIZE = (1080, 1080)

# üìå –®—Ä–∏—Ñ—Ç—Ç—ñ –¥“±—Ä—ã—Å—Ç–∞—É
plt.rcParams["mathtext.fontset"] = "stix"
plt.rcParams["font.weight"] = "bold"  # üèã –ñ–∏—Ä–Ω—ã–π (“õ–∞–ª—ã“£) —à—Ä–∏—Ñ—Ç


@bot.message_handler(commands=["start", "help"])
def send_welcome(message):
    bot.reply_to(message, "–°”ô–ª–µ–º, –±–æ–ª–∞—à–∞“õ –≥—Ä–∞–Ω—Ç –∏–µ–≥–µ—Ä—ñ! “ö–∞–π —Ñ–æ—Ä–º—É–ª–∞–Ω—ã –±—ñ–ª–≥—ñ“£ –∫–µ–ª–µ–¥—ñ?")

@bot.message_handler(func=lambda message: message.text.lower() in formulas)
def send_formula(message):
    formula_text = formulas[message.text.lower()]

    # üìå –§–æ–Ω –∂–∞—Å–∞—É
    img = Image.new("RGB", IMG_SIZE, BACKGROUND_COLOR)

    # üìå –§–æ—Ä–º—É–ª–∞ –∂–∞–∑—É
    fig, ax = plt.subplots(figsize=(8, 4))  # “∞–∑—ã–Ω —Ñ–æ—Ä–º—É–ª–∞–ª–∞—Ä“ì–∞ –∫–µ“£—ñ—Ä–µ–∫ –æ—Ä—ã–Ω –±–µ—Ä—É
    fig.patch.set_facecolor(BACKGROUND_COLOR)
    ax.set_facecolor(BACKGROUND_COLOR)

    ax.text(0.5, 0.5, f"${formula_text}$", fontsize=15, ha='center', va='center', color="white")  # üìå –®—Ä–∏—Ñ—Ç “Ø–ª–∫–µ–Ω
    ax.axis('off')

    # üìå –®–µ–∫–∞—Ä–∞–ª–∞—Ä–¥—ã –∂–æ—é
    fig.subplots_adjust(left=0, right=1, top=1, bottom=0)

    # üìå –°—É—Ä–µ—Ç–∫–µ –∞–π–Ω–∞–ª–¥—ã—Ä—É
    buf = io.BytesIO()
    plt.savefig(buf, format='png', dpi=300, transparent=True)
    buf.seek(0)

    formula_img = Image.open(buf)

    # üìå –§–æ—Ä–º—É–ª–∞–Ω—ã –æ—Ä—Ç–∞—Å—ã–Ω–∞ “õ–æ—é
    formula_x = (IMG_SIZE[0] - formula_img.width) // 2
    formula_y = (IMG_SIZE[1] - formula_img.height) // 2
    img.paste(formula_img, (formula_x, formula_y), formula_img)

    # üìå –õ–æ–≥–æ—Ç–∏–ø “õ–æ—Å—É (–±–æ–ª—Å–∞)
    try:
        logo = Image.open("logo.png").convert("RGBA")
        logo_size = (200, 80)  # –õ–æ–≥–æ—Ç–∏–ø ”©–ª—à–µ–º—ñ
        logo = logo.resize(logo_size)

        x_position = (IMG_SIZE[0] - logo_size[0]) // 2
        y_position = IMG_SIZE[1] - logo_size[1] - 50  # –¢”©–º–µ–Ω–≥—ñ –∂–∞“õ“õ–∞ “õ–æ—é

        img.paste(logo, (x_position, y_position), logo)
    except Exception as e:
        print(f"–õ–æ–≥–æ—Ç–∏–ø—Ç—ñ –∂“Ø–∫—Ç–µ—É “õ–∞—Ç–µ—Å—ñ: {e}")

    # üìå –î–∞–π—ã–Ω —Å—É—Ä–µ—Ç—Ç—ñ –∂—ñ–±–µ—Ä—É
    final_buf = io.BytesIO()
    img.save(final_buf, format="PNG")
    final_buf.seek(0)

    bot.send_photo(message.chat.id, final_buf)
    plt.close()

@bot.message_handler(func=lambda message: message.text.lower() not in formulas)
def unknown_formula(message):
    bot.reply_to(message, "–ö–µ—à—ñ—Ä—ñ“£—ñ–∑, –º–µ–Ω—ñ“£ –∫—ñ—Ç–∞–ø—Ö–∞–Ω–∞–º–¥–∞ –±“±–ª —Ñ–æ—Ä–º—É–ª–∞ —Ç–∞–±—ã–ª–º–∞–¥—ã.")

bot.infinity_polling(timeout=10, long_polling_timeout=5)
